{{#if includeHtml5Library}}<script src="{{scriptUrl}}"></script>{{/if}}
<object id="{{playerId}}" name="{{playerId}}" type="application/x-shockwave-flash" allowFullScreen="true" allowNetworking="all" allowScriptAccess="always" height="{{height}}" width="{{width}}" bgcolor="#000000"{{{elAttributes attributes}}} data="{{swfUrl}}">
	<param name="allowFullScreen" value="true" />
	<param name="allowNetworking" value="all" />
	<param name="allowScriptAccess" value="always" />
	<param name="bgcolor" value="#000000" />
	<param name="flashVars" value="{{{flashVarsString flashVars}}}" />
	<param name="movie" value="{{swfUrl}}" />
	{{#if includeSeoMetadata}}
	<a rel="media:thumbnail" href="{{entryMeta.thumbnailUrl}}"></a>
	<span property="dc:description" content="{{entryMeta.description}}"></span>
	<span property="media:title" content="{{entryMeta.name}}"></span>
	<span property="media:width" content="{{width}}"></span>
	<span property="media:height" content="{{height}}"></span>
	<span property="media:type" content="application/x-shockwave-flash"></span>	
	{{/if}}{{{kalturaLinks}}}
</object>